create table spp_price_predicted 
(
    id                 int unsigned auto_increment primary key,
    route_id           int unsigned                        null,
    pre_price           decimal(10, 2)                  null,
    adj_price           decimal(10, 2)                  null,
    
    inserted_at        timestamp default CURRENT_TIMESTAMP not null,
    last_updated_at    timestamp default CURRENT_TIMESTAMP not null on update CURRENT_TIMESTAMP,
    constraint ride_ibfk_1
        foreign key (route_id) references spp_route (id)
);